    "watchify": "^3.7.0"
  },
  "dependencies": {
    "csjs-inject": "^1.0.0",
    "dom-console": "^3.1.0",
    "yo-yo": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serapath-archive/demo-ca.git"
  },

